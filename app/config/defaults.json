{
  "server": {
    "port": 3000
  },
  "App": {
    "namespace": "/api",
    "version": "/v1"
  },
  "logger": {
    "name": "event-emitter",
    "streams": [{
      "level": "error",
      "path": "logs/error.log"
    }, {
      "type": "rotating-file",
      "path": "logs/dev.log",
      "period": "1d",
      "count": 7
    }],
    "rotationEnabled": true
  }
}
